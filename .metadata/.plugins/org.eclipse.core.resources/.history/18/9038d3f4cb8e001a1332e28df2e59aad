package config;

import org.apache.log4j.Logger;

public class Loggs {
	
	static Logger logger;
	public static void  infolog(String text)
	{
		logger= logger.getLogger("devpinoyLogger");
		logger.info(text);
		
	}
	public static void  errorlog(String text)
	{
		logger= logger.getLogger("devpinoyLogger");
		logger.error(text);
		
	}

}
